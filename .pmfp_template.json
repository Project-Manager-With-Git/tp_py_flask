{
    "language": "py",
    "env": "venv",
    "mini_language_version": "3.8",
    "description": "flask的相关素材库",
    "author": "hsz",
    "template_type": "socket",
    "requires": [
        "flask >= 2.2.2",
        "jsonschema >= 3.2.0",
        "schema_entry >= 0.1.5",
        "pyloggerhelper >= 0.0.2",
        "gevent >=22.10.2",
        "flasgger>=0.9.5",
        "Flask-Cors>=3.0.10"
    ],
    "test_requires": [
        "coverage >= 5.5",
        "mypy >= 0.800",
        "autopep8 >= 1.5.6",
        "pylint >= 2.8.0",
        "pydocstyle>=6.0.0",
        "requests >= 2.25.1"
    ],
    "template_keys": {
        "app_name": {
            "description": "",
            "default": "{{ project_name }}"
        },
        "author": {
            "description": "作者",
            "default": "{{ author }}"
        },
        "author_email": {
            "description": "作者",
            "default": "{{ author_email }}"
        },
        "version": {
            "description": "项目版本",
            "default": "{{ version }}"
        },
        "description": {
            "description": "项目说明",
            "default": "{{ description }}"
        },
        "url":{
            "description": "项目地址",
            "default": "{{ url }}"
        },
        "source": {
            "description": "资源名",
            "default": "example"
        }
    },
    "components": {
        "rest": {
            "source": "rest",
            "description": "RESTful接口模块模板",
            "default_path": "{{ project_name }}/rest"
        },
        "rest_source": {
            "source": "rest/_ref_source_fer__source",
            "description": "RESTfulapi的复杂接口实现模板",
            "default_path": "{{ project_name }}/rest/{{ source }}_source"
        },
        "sse": {
            "source": "sse",
            "description": "sse接口模块模板",
            "default_path": "{{ project_name }}/sse"
        },
        "sse_source": {
            "source": "sse/_ref_source_fer__source.py.jinja",
            "description": "RESTfulapi的复杂接口实现模板",
            "default_path": "{{ project_name }}/sse/{{ source }}_source.py"
        },
        "ws": {
            "source": "ws",
            "description": "",
            "default_path": "ws"
        },
        "ws_source": {
            "source": "ws/_ref_source_fer__source.py.jinja",
            "description": "RESTfulapi的复杂接口实现模板",
            "default_path": "{{ project_name }}/ws/{{ source }}_source.py"
        },
        "main": {
            "source": "main.py.jinja",
            "description": "flask项目的入口模块组件",
            "default_path": "{{ project_name }}/main.py"
        },
        "dockerfile": {
            "source": "dockerfile.jinja",
            "description": "构造flask项目的dockerfile",
            "default_path": "dockerfile"
        },
        "decorator": {
            "source": "decorator",
            "description": "装饰器组件",
            "default_path": "{{ project_name }}/decorator"
        },
        "decorator_source": {
            "source": "decorator/_ref_source_fer__decorator.py.jinja",
            "description": "装饰器文件模版",
            "default_path": "{{ project_name }}/decorator/{{ source }}_decorator.py.jinja"
        },
        "download": {
            "source": "download",
            "description": "大文件下载",
            "default_path": "{{ project_name }}/download"
        },
        "download_source": {
            "source": "download/_ref_source_fer__file.py.jinja",
            "description": "装饰器文件模版",
            "default_path": "{{ project_name }}/download/{{ source }}_file.py.jinja"
        },
        "view": {
            "source": "view",
            "description": "大文件下载",
            "default_path": "{{ project_name }}/download"
        },
        "view_source": {
            "source": "view/_ref_source_fer__view.py.jinja",
            "description": "装饰器文件模版",
            "default_path": "{{ project_name }}/view/{{ source }}_view.py.jinja"
        },
        "servapi": {
            "source": "servs/serv_api",
            "description": "包含RESTful接口,sse接口,和websocket接口的服务模版",
            "default_path": "{{ project_name }}/serv"
        },
        "servmvc": {
            "source": "servs/serv_mvc",
            "description": "由jinja2模版动态渲染的应用模版",
            "default_path": "{{ project_name }}/serv"
        },
        "servmvcapi": {
            "source": "servs/serv_mvcapi",
            "description": "由jinja2模版动态渲染的应用模版同时包含RESTful接口,sse接口,和websocket接口的服务模版",
            "default_path": "{{ project_name }}/serv"
        },
        "servrest": {
            "source": "servs/serv_rest",
            "description": "RESTfulapi服务模版",
            "default_path": "{{ project_name }}/serv"
        },
        "servspa": {
            "source": "servs/serv_spa",
            "description": "混合RESTful,ws,sse和download的单页应用模版",
            "default_path": "{{ project_name }}/serv"
        },
        "servstatic": {
            "source": "servs/serv_static",
            "description": "静态资源服务模板",
            "default_path": "{{ project_name }}/serv"
        },
        "servws": {
            "source": "servs/serv_ws",
            "description": "websocket服务模版",
            "default_path": "{{ project_name }}/serv"
        }
    }
}